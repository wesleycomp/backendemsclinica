    "exceljs": "4.4.0",
    "express": "4.17.2",
    "express-async-errors": "3.1.1",
    "ioredis": "4.30.0",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "module-alias": "^2.2.3",
    "multer": "1.4.5-lts.2",
